          "_customHTML": "<section data-bs-version=\"5.1\" mbr-id=\"_slider_with_tab\" class=\"mbr-section slider_with_text_button\" mbr-class=\"{\n         'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n      <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"0\">\n      <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"0\">\n      <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#f4f1f1\">\n      <input type=\"color\" title=\"Title Bg Color\" name=\"tbgColor\" value=\"#064800\">\n      <input type=\"color\" title=\"SubTitle Bg Color\" name=\"stbgColor\" value=\"#000326\">\n      <input type=\"color\" title=\"Indicator Bg Color\" name=\"inbgColor\" value=\"#ff0000\">\n      <input type=\"checkbox\" title=\"Show Title\" name=\"showtitle\">\n      <input type=\"checkbox\" title=\"Show Sub Title\" name=\"showsubtitle\" checked>\n      <input type=\"checkbox\" title=\"Show Navigator\" name=\"shownavi\" checked>\n      <select title=\"Content Width\" name=\"widthContent\">\n            <option value=\"1\" selected>Full Width</option>\n            <option value=\"2\">None Full Width</option>\n      </select>\n      <select title=\"Image Ratio\" name=\"imgratio\" mbr-if=\"widthContent == '1'\">\n            <option value=\"fill\" selected>Fill</option>\n            <option value=\"contain\">Contain</option>\n            <option value=\"cover\">Cover</option>\n        <option value=\"scale-down\">Scale down</option>\n                <option value=\"none\">None</option>\n      </select>\n      <select title=\"Slider Count\" name=\"slidercount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n\t\t\t      <option value=\"4\">4</option>\n\t\t\t      <option value=\"5\">5</option>\n            <option value=\"6\" selected>6</option>\n      </select>\n    <!-- End block parameters -->\n    </mbr-parameters> \n\n <div mbr-class=\"{ 'fullwidth': widthContent == 1, 'container': widthContent == 2}\">  \n\n\t\t\t<div id=\"myCarousel2singa\" class=\"carousel slide\" data-ride=\"carousel\" data-bs-ride=\"carousel\">\n\t\t\t\t<!-- Wrapper for carousel items -->\n\t\t\t\t<div class=\"carousel-inner\">\n\t\t\t\t\t<div class=\"item carousel-item active\" mbr-if=\"slidercount > 0\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/mbr-9.jpg\" style=\"object-fit:{{imgratio}}\" alt=\"Garage Cars\">\n\t\t\t\t\t\t<div class=\"carousel-caption\">\n\t\t\t\t\t\t\t<h3 mbr-if=\"showtitle\"><b>Đào Tạo Thiết Kế Website</b></h3>\n\t\t\t\t\t\t\t<p mbr-if=\"showsubtitle\"><b>Chuyên Làm Đồng Khung Xe ÔTô</b></p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\t\n\t\t\t\t\t<div class=\"item carousel-item\" mbr-if=\"slidercount > 1\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/mbr-10.jpg\" style=\"object-fit:{{imgratio}}\" alt=\"Garage Cars\">\n\t\t\t\t\t\t<div class=\"carousel-caption\">\n\t\t\t\t\t\t\t<h3 mbr-if=\"showtitle\"><b>Thuê Bao QR WEBSITE</b></h3>\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<p mbr-if=\"showsubtitle\"><b>Sửa Chữa Đại Tu Động Cơ Xe ÔTô</b></p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item carousel-item\" mbr-if=\"slidercount > 2\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/mbr-11.jpg\" style=\"object-fit:{{imgratio}}\" alt=\"Garage Cars\">\n\t\t\t\t\t\t<div class=\"carousel-caption\">\n\t\t\t\t\t\t\t<h3 mbr-if=\"showtitle\"><b>Chuyển Giao Công Nghệ Web 4.0</b></h3>\n\t\t\t\t\t\t\t<p mbr-if=\"showsubtitle\"><b>Sửa Chữa Điều Hòa Xe ÔTô</b></p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item carousel-item\" mbr-if=\"slidercount > 3\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/mbr-3.jpg\" style=\"object-fit:{{imgratio}}\" alt=\"Garage Cars\">\n\t\t\t\t\t\t<div class=\"carousel-caption\">\n\t\t\t\t\t\t\t<h3 mbr-if=\"showtitle\"><b>Chuyển Đổi Số Doanh Nghiệp</b></h3>\n\t\t\t\t\t\t\t<p mbr-if=\"showsubtitle\"><b>Chuyên Bảo Dưỡng Xe ÔTô Định Kỳ</b></p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item carousel-item\" mbr-if=\"slidercount > 4\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/mbr-4.jpg\" style=\"object-fit:{{imgratio}}\" alt=\"Garage Cars\">\n\t\t\t\t\t\t<div class=\"carousel-caption\">\n\t\t\t\t\t\t\t<h3 mbr-if=\"showtitle\"><b>Gia Công Phần Mềm Máy Tính</b></h3>\n\t\t\t\t\t\t\t<p mbr-if=\"showsubtitle\"><b>Chuyên Sửa Chữa Gầm Máy ÔTô</b></p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"item carousel-item\" mbr-if=\"slidercount > 5\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/garage-training.jpg\" style=\"object-fit:{{imgratio}}\" alt=\"Garage Cars\">\n\t\t\t\t\t\t<div class=\"carousel-caption\">\n\t\t\t\t\t\t\t<h3 mbr-if=\"showtitle\"><b>Quản Trị Hệ Thống Mạng Máy Tính</b></h3>\n\t\t\t\t\t\t\t<p mbr-if=\"showsubtitle\"><b>Nhận Hồ Sơ Dạy Lái &amp; Bảo Dưỡng ÔTô</b></p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<!-- End Carousel Inner -->\n\t\t\t\t<div class=\"nav nav-pills nav-justified carousel-indicators\" mbr-if=\"slidercount > 0\">\n\t\t\t\t\t<div data-target=\"#myCarousel2singa\" data-bs-target=\"#myCarousel2singa\" data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"nav-item active\" mbr-if=\"slidercount > 0\" mbr-text=\"\"><a href=\"#\" class=\"nav-link\"><strong>LÀM ĐỒNG</strong>Làm Đồng Khung Xe ÔTô</a></div>\n\t\t\t\t\t<div data-target=\"#myCarousel2singa\" data-bs-target=\"#myCarousel2singa\" data-slide-to=\"1\" data-bs-slide-to=\"1\" class=\"nav-item\" mbr-if=\"slidercount > 1\" mbr-text=\"\"><a href=\"#\" class=\"nav-link\"><strong>ĐỘNG CƠ</strong>Đại Tu Động Cơ ÔTô</a></div>\n\t\t\t\t\t<div data-target=\"#myCarousel2singa\" data-bs-target=\"#myCarousel2singa\" data-slide-to=\"2\" data-bs-slide-to=\"2\" class=\"nav-item\" mbr-if=\"slidercount > 2\" mbr-text=\"\"><a href=\"#\" class=\"nav-link\"><strong>ĐIỀU HÒA</strong>Chuyên Điều Hòa ÔTô</a></div>\n\t\t\t\t\t<div data-target=\"#myCarousel2singa\" data-bs-target=\"#myCarousel2singa\" data-slide-to=\"3\" data-bs-slide-to=\"3\" class=\"nav-item\" mbr-if=\"slidercount > 3\" mbr-text=\"\"><a href=\"#\" class=\"nav-link\"><strong>BẢO DƯỠNG</strong>Bảo Dưỡng ÔTô Định Kỳ</a></div>\n\t\t\t\t\t<div data-target=\"#myCarousel2singa\" data-bs-target=\"#myCarousel2singa\" data-slide-to=\"4\" data-bs-slide-to=\"4\" class=\"nav-item\" mbr-if=\"slidercount > 4\" mbr-text=\"\"><a href=\"#\" class=\"nav-link\"><strong>GẦM MÁY</strong>Sửa Chữa Gầm Máy ÔTô</a></div>\n\t\t\t\t\t<div data-target=\"#myCarousel2singa\" data-bs-target=\"#myCarousel2singa\" data-slide-to=\"5\" data-bs-slide-to=\"5\" class=\"nav-item\" mbr-if=\"slidercount > 5\" mbr-text=\"\"><a href=\"#\" class=\"nav-link\"><strong>ĐÀO TẠO</strong>Đào Tạo Nghề ÔTô</a></div>\n\t\t\t\t</div>\n              <!-- Carousel controls -->\n\t\t\t\t<a class=\"carousel-control left carousel-control-prev\" href=\"#myCarousel2singa\" data-slide=\"prev\" data-bs-slide=\"prev\" mbr-if=\"slidercount > 1 &amp;&amp; shownavi\">\n\t\t\t\t\t<i class=\"fa fa-angle-left fas\"></i>\n\t\t\t\t</a>\n\t\t\t\t<a class=\"carousel-control right carousel-control-next\" href=\"#myCarousel2singa\" data-slide=\"next\" data-bs-slide=\"next\" mbr-if=\"slidercount > 1 &amp;&amp; shownavi\">\n\t\t\t\t\t<i class=\"fa fa-angle-right fas\"></i>\n\t\t\t\t</a>\n\t\t\t</div>\n\t\t\n\t</div>\n \n  \n</section>",
